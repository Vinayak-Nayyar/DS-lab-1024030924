#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

struct Edge {
    int u, v, w;
};

vector<Edge> edges;
int parent[100];

int findSet(int x) {
    if (parent[x] == x) return x;
    return parent[x] = findSet(parent[x]);
}

void uni(int a, int b) {
    parent[findSet(a)] = findSet(b);
}

int main() {
    int n, e;
    cin >> n >> e;

    for (int i = 1; i <= n; i++) parent[i] = i;

    for (int i = 0; i < e; i++) {
        int u, v, w;
        cin >> u >> v >> w;
        edges.push_back({u, v, w});
    }

    sort(edges.begin(), edges.end(),
         [](Edge a, Edge b){ return a.w < b.w; });

    int mst = 0;
    for (auto ed : edges) {
        if (findSet(ed.u) != findSet(ed.v)) {
            mst += ed.w;
            uni(ed.u, ed.v);
        }
    }

    cout << mst;
}
