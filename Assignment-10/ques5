#include <iostream>
#include <vector>
#include <queue>
using namespace std;

vector<pair<int,int>> adj[100];
bool visited[100];

int main() {
    int n, e;
    cin >> n >> e;

    for (int i = 0; i < e; i++) {
        int u, v, w;
        cin >> u >> v >> w;
        adj[u].push_back({v, w});
        adj[v].push_back({u, w});
    }

    priority_queue<pair<int,int>,
               vector<pair<int,int>>,
               greater<pair<int,int>>> pq;

    pq.push({0, 1});
    int mst = 0;

    while (!pq.empty()) {
        auto [w, u] = pq.top();
        pq.pop();

        if (visited[u]) continue;
        visited[u] = true;
        mst += w;

        for (auto [v, wt] : adj[u])
            if (!visited[v])
                pq.push({wt, v});
    }

    cout << mst;
}
