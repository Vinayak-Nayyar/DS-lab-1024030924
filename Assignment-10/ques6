#include <iostream>
#include <vector>
#include <queue>
using namespace std;

vector<pair<int,int>> adj[100];
int dist[100];

int main() {
    int n, e;
    cin >> n >> e;

    for (int i = 1; i <= n; i++) dist[i] = 1e9;

    for (int i = 0; i < e; i++) {
        int u, v, w;
        cin >> u >> v >> w;
        adj[u].push_back({v, w});
    }

    int start;
    cin >> start;

    priority_queue<pair<int,int>,
        vector<pair<int,int>>,
        greater<pair<int,int>>> pq;

    dist[start] = 0;
    pq.push({0, start});

    while (!pq.empty()) {
        auto [d, u] = pq.top();
        pq.pop();

        for (auto [v, w] : adj[u]) {
            if (dist[v] > d + w) {
                dist[v] = d + w;
                pq.push({dist[v], v});
            }
        }
    }

    for (int i = 1; i <= n; i++)
        cout << dist[i] << " ";
}
