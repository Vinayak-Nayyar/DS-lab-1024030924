#include <iostream>
#include <vector>
using namespace std;

vector<int> adj[10];
int n, e;

int main() {
    cin >> n >> e;

    for (int i = 0; i < e; i++) {
        int u, v;
        cin >> u >> v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }

    int x;
    cin >> x;

    cout << "Degree: " << adj[x].size() << endl;

    cout << "Adjacent vertices: ";
    for (int v : adj[x]) cout << v << " ";
    cout << endl;

    cout << "Number of edges: " << e << endl;
}
