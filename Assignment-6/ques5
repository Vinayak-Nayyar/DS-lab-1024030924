#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

Node* head = NULL;

bool isCircular() {
    if (head == NULL) return false;
    Node* temp = head->next;
    while (temp != NULL && temp != head)
        temp = temp->next;
    return temp == head;
}

int main() {
    Node* a = new Node{10, NULL};
    Node* b = new Node{20, NULL};
    Node* c = new Node{30, NULL};

    head = a;
    a->next = b;
    b->next = c;


    if (isCircular())
        cout << "Circular Linked List";
    else
        cout << "Not Circular";
}
